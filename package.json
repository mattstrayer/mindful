{
  "name": "mindful",
  "displayName": "mindful",
  "version": "0.0.1",
  "description": "A basic extension.",
  "author": "matt strayer <git@mattstrayer.com>",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "lint": "eslint ./src --ext .vue,.ts,.js  --ignore-path .gitignore",
    "lint:fix": "eslint ./src --ext .vue,.ts,.js --fix --ignore-path .gitignore",
    "format": "prettier . --check",
    "format:fix": "prettier --write .",
    "lf:fix": "npm-run-all -c lint:fix format:fix"
  },
  "dependencies": {
    "@types/webextension-polyfill": "0.10.7",
    "@vueuse/core": "10.9.0",
    "@vueuse/motion": "2.1.0",
    "broadcast-channel": "7.0.0",
    "i": "0.3.7",
    "npm": "10.5.0",
    "pinia": "2.1.7",
    "pinia-orm": "1.7.2",
    "pinia-plugin-persistedstate": "3.2.1",
    "uuid": "9.0.1",
    "vue": "3.4.21"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@rushstack/eslint-patch": "1.7.2",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.11.24",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "autoprefixer": "10.4.18",
    "daisyui": "4.7.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-vue": "9.22.0",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "typescript-eslint": "7.1.1",
    "vue-tsc": "2.0.5",
    "wxt": "0.17.7"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage",
      "webRequest",
      "webNavigation"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*",
      "activeTab"
    ]
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.5.0"
  }
}
